  "scripts": {
    "env": "dotenv -e .env -e .env.local --",
    "preuni": "node bin/views-checker.js",
    "uni": "yarn env uni",
    "uni:build": "vue-tsc --noEmit && npm --no-git-tag-version version minor && yarn uni build",
    "dev": "yarn uni",
    "devw": "bash bin/wx-dev.sh",
    "build": "yarn uni:build",
    "buildw": "yarn uni:build -p mp-weixin",
    "r": "yarn release",
    "rm": "yarn release:migrate",
    "rc": "yarn build && yarn release",
    "commit": "git add . && git commit -am ",
    "cm": "yarn commit",
    "push": "yarn commit",
    "postpush": "while true; do git push origin master && { echo 'Git push succeeded'; break; } || echo 'Git push failed, retrying in 1 seconds'; sleep 1; done",
    "pull": "while true; do git pull origin master && { echo 'Git pull succeeded'; break; } || echo 'Git pull failed, retrying in 1 seconds'; sleep 1; done",
    "f": "yarn find -print0| xargs -0 grep -n ",
    "npm:reset": "rm -rf node_modules && npm ci",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "postinstall": "patch-package",
    "pu": "npx patch-package @dcloudio/uni-ui",
    "pua": "npm remove @dcloudio/uni-ui && npm install @dcloudio/uni-ui",
    "type-check": "vue-tsc --noEmit"
  },
